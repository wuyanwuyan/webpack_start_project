{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    [
      "stage-0"
    ]
  ],
  "plugins": [
    "transform-runtime",
    "transform-node-env-inline"
  ],
  "ignore": [
    "server/static/"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-runtime",
        "transform-node-env-inline",
        "transform-remove-console"
      ]
    }
  }
}
